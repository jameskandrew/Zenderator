{% for argName,arg in args %}
{% if arg.subType == "AbstractModel" %}
{% if arg.format == "full"%}
{% set suffix = "->__toArray()" %}
{% elseif arg.format == "diff" %}
{% set suffix = "->__toDirtyArray()" %}
{% else %}
{% set suffix = "" %}
{% endif %}
{% else %}
{% set suffix = "" %}
{% endif %}
{% if argName == "body" and name == "bodyArgs" %}
/*
{{ arg|json_encode(constant('JSON_PRETTY_PRINT'))|raw }}
*/
        ${{ name }} = ${{ argName }}{{ suffix|raw }};
{% else %}
        ${{ name }}["{{ argName }}"] = ${{ argName }}{{ suffix|raw }};
{% endif %}
{% endfor %}
