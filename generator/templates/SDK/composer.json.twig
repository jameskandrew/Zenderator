{
    "name": "{{ namespace|lower }}/lib{{ appName|lower }}",
    "authors": [
        {
            "name": "Matthew Baggett",
            "email": "matt.baggett@segura.co.uk"
        },
        {
            "name": "Segura Systems",
            "email": "systems@segura.co.uk"
        }
    ],
    "license": "proprietary",
    "time": "{{ releaseTime }}",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:segurasystems/libCommon.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:segurasystems/lib{{ appName }}Models.git"
        }
    ],
    "require": {
        "gone.io/libcommon": "dev-master",
        "segura/lib{{ appName|lower }}models": "dev-master",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "kint-php/kint": "^2.0",
        "fzaninotto/Faker": "^1.8",
        "phpunit/phpunit": "^7.3",
        "php-vcr/phpunit-testlistener-vcr": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "{{ classNamespaceJSONSAFE }}\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "{{ classNamespaceJSONSAFE }}\\Test\\": "tests/"
        }
    }
}
