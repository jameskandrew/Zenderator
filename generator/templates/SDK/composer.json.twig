{
    "name": "segura/lib{{ app_name|lower }}",
    "authors": [
        {
            "name": "Matthew Baggett",
            "email": "matt.baggett@segura.co.uk"
        },
        {
            "name": "Segura Systems",
            "email": "systems@segura.co.uk"
        }
    ],
    "license": "proprietary",
    "time": "{{ release_time }}",
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:segurasystems/libCommon.git"
        }
    ],
    "require": {
        "segura/libcommon": "dev-master"
    },
    "require-dev": {
        "kint-php/kint": "^2.0",
        "fzaninotto/Faker": "^1.8",
        "phpunit/phpunit": "^7.3",
        "php-vcr/phpunit-testlistener-vcr": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Gone\\SDK\\{{ app_name }}\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gone\\SDK\\{{ app_name }}\\Test\\": "tests/"
        }
    }
}
